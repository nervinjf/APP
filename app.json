{
  "expo": {
    "name": "StartUp2",
    "slug": "StartUp2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoStartUp.png",
    "userInterfaceStyle": "night",
    "splash": {
      "image": "./assets/logoStartUp.png",
      "resizeMode": "contain",
      "backgroundColor": "#7B34DC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      },
      "bundleIdentifier": "com.nervinjesus.StartUp2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.nervinjesus.StartUp2"
    },
    "scheme": "StartUp2",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5c035b55-04b2-4158-9a96-932d7b5c627c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5c035b55-04b2-4158-9a96-932d7b5c627c"
    }
  }
}
